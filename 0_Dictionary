proc;
run;

proc means; // number type
var
run;

proc frcq; // text type
table
run;

proc sql;  // sql querys
select..
quit;
 
proc univariate; // numeric variable analysis (mean, std dev, percentiles, normality tests, plots)
var;
run;


desc (macro parameter)// 	The variable name to analyze, passed by you to the macro
	// Example call 	%MyStats(lib=sashelp, data=cars, desc=msrp, stats=univariate)
	
	
